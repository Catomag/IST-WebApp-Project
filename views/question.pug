doctype 5
html
  head
    title Webzone Name
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet" type="text/css" href="/css/styles.css")
    script(src="/js/helper.js")
    script(src="/js/serverConnect.js")
  body
    center
      div
        div(id="floater")
        div(id="question" class="verticalCentre")
          form(onsubmit="createHost(); return false")
            input(type="text" id="questionInput" maxlength="40" style="padding-left: 10px")
      p(id="hostId")
    div
      span(onclick="confirm('clicked')" id="plus" class="circle")
        img(src="/img/Plus.png" width="50px" height="50px" style="margin: 20px;" )
